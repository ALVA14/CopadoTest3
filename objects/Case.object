    <webLinks>
        <fullName>LiveText__Send_Bulk_SMS</fullName>
        <availability>online</availability>
        <description>Sends text message to selected records</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Text to List</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)} 
            {!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 
            {!REQUIRESCRIPT(&apos;/support/console/31.0/integration.js&apos;)}; 

            var records = {!GETRECORDIDS($ObjectType.Case)}; 
            var hasShortCodeNumber = sforce.apex.execute(&quot;LiveText.RetrievePreferredPhoneNumber&quot;,&quot;agentHasShortcodeNumberAssigned&quot;,{});
            if (records[0] == null){ 
                alert(&quot;Please select at least one case&quot;);
            }else if((records.length &gt; 25) &amp;&amp; hasShortCodeNumber==&quot;false&quot;){ 
                alert(&quot;A maximum of 25 text messages may be sent at once. Please select fewer recipients and try again.&quot;); 
            } 
            else{ 
                var packagePrefix = &quot;LiveText__&quot;; 
                var recordType = &quot;case&quot;; 
                var recordIds = &quot;&quot;;

                for (var n in records) { 
                    if(n == &apos;remove&apos; || n == &apos;indexOf&apos;) continue; 
                    recordIds = recordIds + records[n]; 
                    
                    if (n != records.length-1) { 
                        recordIds = recordIds + &quot;,&quot;; 
                    } 
                }

                var storageId = sforce.apex.execute(&quot;LiveText.RetrievePreferredPhoneNumber&quot;,&quot;saveToStorage&quot;,{stringToSave:recordIds});
                var urlStart = &apos;https://&apos;+window.location.hostname+&apos;/apex/LiveText__LiveTextBulkSms?recordType=&apos;+recordType+&apos;&amp;storageId=&apos;+storageId; 
                
                if(sforce.console.isInConsole()) { 
                    sforce.console.openPrimaryTab(null, urlStart, true);
                } else { 
                    window.top.location.href = urlStart; 
                } 
            }</url>
    </webLinks>
    <webLinks>
        <fullName>Send_Message</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Message</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/31.0/connection.js&apos;)}; 
{!REQUIRESCRIPT(&apos;/soap/ajax/31.0/apex.js&apos;)}; 
{!REQUIRESCRIPT(&apos;/support/console/31.0/integration.js&apos;)}; 

var objectId = &quot;{!Case.ContactId}&quot;;
var contactId = &quot;{!Case.ContactId}&quot;;
var caseId = &quot;{!Case.Id}&quot;;
var caseIsClosed = {!Case.IsClosed};
var caseNumber = &quot;{!Case.CaseNumber}&quot;;
var accountId = &quot;{!Case.AccountId}&quot;;
if (accountId || contactId) {
    var results = sforce.connection.query(
        &apos;SELECT Id, LiveText__Contact__c, LiveText__Account__c, LiveText__Case__c &apos;
        + &apos;FROM LiveText__Conversation_Header__c &apos;
        + &apos;WHERE (LiveText__Contact__c = \&apos;&apos; + contactId + &apos;\&apos; OR LiveText__Account__c = \&apos;&apos; + accountId + &apos;\&apos;) AND LiveText__Status__c = \&apos;Active\&apos;&apos;
    );

    var sessions = (results.size + &apos;&apos;) === &apos;1&apos; ? [results.records] : results.records;

    if (caseIsClosed) {
        alert(&quot;{!$Label.Case_SendMessage_ClosedCase}&quot;);
    } else if (sessions &amp;&amp; sessions.length) {
        alert(&quot;{!$Label.Case_SendMessage_ActiveSession}&quot;);
    } else {
var recordPrefix = objectId.substring(0, 3); 
var socialPersonaInformation = JSON.parse(sforce.apex.execute(&quot;LiveText.RetrieveSocialPersona&quot;,&quot;getSocialPersonaInfo&quot;,{selectedObjId:objectId})); 
if (!socialPersonaInformation || socialPersonaInformation == null) {
    objectId = accountId;
    recordPrefix = objectId.substring(0, 3); 
    socialPersonaInformation = JSON.parse(sforce.apex.execute(&quot;LiveText.RetrieveSocialPersona&quot;,&quot;getSocialPersonaInfo&quot;,{selectedObjId:objectId}));
} 
if (!socialPersonaInformation || socialPersonaInformation == null) 
{ 
var phoneInformation = JSON.parse(sforce.apex.execute(&quot;LiveText.RetrievePreferredPhoneNumber&quot;,&quot;getToPhoneInformationFromObject&quot;,{selectedObjId:objectId})); 
var phoneNumber = phoneInformation[&apos;phone&apos;]; 
var oObjectId = phoneInformation[&apos;orginatingObjectId&apos;]; 
var ownerId = phoneInformation[&apos;ownerId&apos;]; 
var params = &apos;&amp;phone=&apos;+phoneNumber.replace(&apos;+&apos;, &apos;%2B&apos;) +&apos;&amp;oid=&apos;+objectId+&apos;&amp;conversationtype=outbound&amp;objectprefix=&apos;+recordPrefix+&apos;&amp;ownerId=&apos;+ownerId+&apos;&amp;orginatingObjectId=&apos;+oObjectId; 
var conversationSessionPrefix = &apos;/{!$ObjectType.LiveText__Conversation_Session__c}/e?caseId=&apos;+caseId+&apos;&amp;caseNumber=&apos;+caseNumber+&apos;&amp;accountId=&apos;+accountId+&apos;&amp;cvid=null&apos;+params; 

if(sforce.console.isInConsole()){ 
sforce.console.openPrimaryTab(null, conversationSessionPrefix, true, &apos;{!HTMLENCODE($Label.livetext__txt_conversation)}&apos;, null); 
var closeSubtab = function closeSubtab(result) { 
var tabId = result.id; 
sforce.console.closeTab(tabId); 
}; 
sforce.console.getEnclosingTabId(closeSubtab); 
}else{ 
var query = &quot;SELECT Id FROM AppMenuItem WHERE Name=&apos;LiveText_Console&apos;&quot;; 
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;; 
var queryResult = sforce.connection.query(query); 
